            </div>
        </div>
    </div>
    <!-- sales report area end -->
    <!-- overview area start -->
    <div class="row">
        <div class="col-lg-12">
            <div class="single-report">
                <div class="s-report-inner pr--20 pt--30 mb-3">
                    <div class="icon"><i class="ti-pie-chart"></i></div>
                    <div class="s-report-title d-flex justify-content-between">
                        <h4 class="header-title mb-0">Games Played</h4>
                        <p>Last 28 Days</p>
                    </div>
                </div>
                <div style="min-height:400px;max-height:400px;">
                    @if (Dashboard.Helpers.ActivityData.Any28D(Dashboard.Helpers.Account.GetUserId(User.Identity.Name)))
                    {
                        <canvas id="games_played" height="150"></canvas>
                    }
                    else
                    {
                        <div class="card" style="height: 300px;vertical-align:middle;">
                            <div class="card-body" style="text-align:center;">
                                <i class="ti-bar-chart" style="font-size:50px;"></i><br />
                                Nothing here
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
    <!-- overview area end -->
    <!-- market value area start -->
    <div class="row mt-5 mb-5">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <div class="d-sm-flex justify-content-between align-items-center">
                        <h4 class="header-title mb-0">Top 3</h4>
                    </div>
