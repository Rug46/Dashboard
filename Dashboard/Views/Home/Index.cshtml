<!-- page title area end -->
<div class="main-content-inner">
    <!-- sales report area start -->
    <div class="sales-report-area mt-5 mb-5">
        <div class="row">
            <div class="col-md-4">
                <div class="single-report mb-xs-30">
                    <div class="s-report-inner pr--20 pt--30 mb-3">
                        <div class="icon"><i class="ti-time"></i></div>
                        <div class="s-report-title d-flex justify-content-between">
                            <h4 class="header-title mb-0">Last 24 Hours</h4>
                            <p>@Dashboard.Data.Data.GetPlayTimeLast24H()</p>
                        </div>
                    </div>
                    <canvas id="hours_today" height="100"></canvas>
                </div>
            </div>
            <div class="col-md-4">
                <div class="single-report mb-xs-30">
                    <div class="s-report-inner pr--20 pt--30 mb-3">
                        <div class="icon"><i class="ti-time"></i></div>
                        <div class="s-report-title d-flex justify-content-between">
                            <h4 class="header-title mb-0">Last 7 Days</h4>
                            <p>@Dashboard.Data.Data.GetPlayTimeLast7D()</p>
                        </div>
                    </div>
                    <canvas id="hours_week" height="100"></canvas>
                </div>
            </div>
            <div class="col-md-4">
                <div class="single-report">
                    <div class="s-report-inner pr--20 pt--30 mb-3">
                        <div class="icon"><i class="ti-time"></i></div>
                        <div class="s-report-title d-flex justify-content-between">
                            <h4 class="header-title mb-0">Last 28 Days</h4>
                            <p>@Dashboard.Data.Data.GetPlayTimeLast28D()</p>
                        </div>

                    </div>
                    <canvas id="hours_month" height="100"></canvas>
                </div>
            </div>
        </div>
    </div>
    <!-- sales report area end -->
    <!-- overview area start -->
    <div class="row">
        <div class="col-lg-12">
            <div class="single-report">
                <div class="s-report-inner pr--20 pt--30 mb-3">
                    <div class="icon"><i class="ti-pie-chart"></i></div>
                    <div class="s-report-title d-flex justify-content-between">
                        <h4 class="header-title mb-0">Games Played</h4>
                        <p>Last 28 Days</p>
                    </div>
                </div>
                <canvas id="games_played" height="150"></canvas>
            </div>
        </div>
    </div>
    <!-- overview area end -->
    <!-- market value area start -->
    <div class="row mt-5 mb-5">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <div class="d-sm-flex justify-content-between align-items-center">
                        <h4 class="header-title mb-0">Top 3</h4>
                    </div>
                    <div class="market-status-table mt-4">
                        <div class="table-responsive">
                            <table class="dbkit-table">
                                <tr class="heading-td">
                                    <td class="mv-icon">Place</td>
                                    <td class="coin-name">Game Name</td>
                                    <td class="buy">Last 24 Hours</td>
                                    <td class="sell">Last 28 Days</td>
                                </tr>
                                <tr>
                                    <td class="mv-icon">
                                        1
                                    </td>
                                    <td class="coin-name">@Dashboard.Data.Data.GetMostPlayedName(1)</td>
                                    <td class="buy">@Dashboard.Data.Data.GetMostPlayedTimeTodayFormatted(1)</td>
                                    <td class="sell">@Dashboard.Data.Data.GetMostPlayedTimeFormatted(1)</td>
                                </tr>
                                <tr>
                                    <td class="mv-icon">
                                        2
                                    </td>
                                    <td class="coin-name">@Dashboard.Data.Data.GetMostPlayedName(2)</td>
                                    <td class="buy">@Dashboard.Data.Data.GetMostPlayedTimeTodayFormatted(2)</td>
                                    <td class="sell">@Dashboard.Data.Data.GetMostPlayedTimeFormatted(2)</td>
                                </tr>
                                <tr>
                                    <td class="mv-icon">
                                        3
                                    </td>
                                    <td class="coin-name">@Dashboard.Data.Data.GetMostPlayedName(3)</td>
                                    <td class="buy">@Dashboard.Data.Data.GetMostPlayedTimeTodayFormatted(3)</td>
                                    <td class="sell">@Dashboard.Data.Data.GetMostPlayedTimeFormatted(3)</td>
                                </tr>
                            </table>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>