                                <div class="card-body" style="text-align:center;">
                                    <i class="ti-bar-chart" style="font-size:50px;"></i><br />
                                    Nothing here
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="single-report">
                    <div class="s-report-inner pr--20 pt--30 mb-3">
                        <div class="icon"><i class="ti-time"></i></div>
                        <div class="s-report-title d-flex justify-content-between">
                            <h4 class="header-title mb-0">Last 28 Days</h4>
                            <p>@Dashboard.Helpers.ActivityData.GetPlayTimeLast28D(Dashboard.Helpers.Account.GetUserId(User.Identity.Name))</p>
                        </div>

                    </div>
                    <div style="min-height:100px;max-height:100px;">
                        @if (Dashboard.Helpers.ActivityData.Any28D(Dashboard.Helpers.Account.GetUserId(User.Identity.Name)))
                        {
                            <canvas id="hours_month" height="100"></canvas>
                        }
                        else
