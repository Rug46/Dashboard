<div class="main-content-inner">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <div>
                        <a class="btn btn-primary" href="/Chart/Year">Back to search</a>
                    </div>
                    <br />
                    @if (TempData["Error"] != null)
                    {
                        <div class="alert alert-danger">
                            @TempData["Error"].ToString()
                        </div>
                    }
                    <h1>@TempData["MonthName"].ToString() @month.Year</h1>

                    @if (monthNum == 2)
                    {
                        <canvas id="monthChart" height="100"></canvas>

                        <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.3/Chart.bundle.min.js"></script>
                        <script src="~/js/ChartSectionMonth28.js"></script>
                    }
                    else if (monthNum == 4 || monthNum == 6 || monthNum == 9 || monthNum == 11)
                    {
                        <canvas id="monthChart" height="100"></canvas>

                        <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.3/Chart.bundle.min.js"></script>
                        <script src="~/js/ChartSectionMonth30.js"></script>
                    }
                    else
                    {
                        <canvas id="monthChart" height="100"></canvas>

                        <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.3/Chart.bundle.min.js"></script>
                        <script src="~/js/ChartSectionMonth.js"></script>
                    }

                    <hr />
                    <form action="/Chart/Month" method="post">
                        Month
                        <select id="month-select" name="month" class="textbox">
                            <option></option>

                            @{
                                var firstRecordDate = Dashboard.Helpers.ActivityDataYears.GetFirstRecordDate(Dashboard.Helpers.Account.GetUserId(User.Identity.Name));
                                var thisMonth = DateTime.Now;
                                var months = Dashboard.Helpers.ActivityDataYears.GetMonths(thisMonth, firstRecordDate);
                            }

                            @for (int i = 0; i < months + 1; i++)
                            {
                                var monthsAgo = DateTime.Now.AddMonths(-i);

                                <option value="@monthsAgo">@Dashboard.Helpers.ActivityDataYears.GetMonthName(monthsAgo)</option>
                            }
                        </select>
                        <input type="submit" class="btn btn-primary padding-btn" value="Go" />
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>