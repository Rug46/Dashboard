<div class="main-content-inner">
    <!-- sales report area start -->
    <div class="sales-report-area mt-5 mb-5">
        <div class="row">
            <div class="col-md-4">
                <div class="single-report mb-xs-30">
                    <div class="s-report-inner pr--20 pt--30 mb-3">
                        <div class="icon"><i class="ti-time"></i></div>
                        <div class="s-report-title d-flex justify-content-between">
                            <h4 class="header-title mb-0">Last 24 Hours</h4>
                            <p>@Dashboard.Helpers.Data.GetPlayTimeLast24H(Dashboard.Helpers.Account.GetUserId(User.Identity.Name))</p>
                        </div>
                    </div>
                    <div style="min-height:100px;max-height:100px;">
                        @if (Dashboard.Helpers.Data.Any24H(Dashboard.Helpers.Account.GetUserId(User.Identity.Name)))
                        {
                            <canvas id="hours_today" height="100"></canvas>
                        }
                        else
                        {
                            <div class="card" style="height: 100px;">
                                <div class="card-body" style="text-align:center;">
                                    <i class="ti-bar-chart" style="font-size:50px;"></i><br />
                                    Nothing here
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="single-report mb-xs-30">
                    <div class="s-report-inner pr--20 pt--30 mb-3">
                        <div class="icon"><i class="ti-time"></i></div>
                        <div class="s-report-title d-flex justify-content-between">
                            <h4 class="header-title mb-0">Last 7 Days</h4>
                            <p>@Dashboard.Helpers.Data.GetPlayTimeLast7D(Dashboard.Helpers.Account.GetUserId(User.Identity.Name))</p>
                        </div>
                    </div>
                    <div style="min-height:100px;max-height:100px;">
                        @if (Dashboard.Helpers.Data.Any7D(Dashboard.Helpers.Account.GetUserId(User.Identity.Name)))
                        {
                            <canvas id="hours_week" height="100"></canvas>
                        }
                        else
                        {
                            <div class="card" style="height: 100px;">
                                <div class="card-body" style="text-align:center;">
                                    <i class="ti-bar-chart" style="font-size:50px;"></i><br />
                                    Nothing here
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="single-report">
                    <div class="s-report-inner pr--20 pt--30 mb-3">
                        <div class="icon"><i class="ti-time"></i></div>
                        <div class="s-report-title d-flex justify-content-between">
                            <h4 class="header-title mb-0">Last 28 Days</h4>
                            <p>@Dashboard.Helpers.Data.GetPlayTimeLast28D(Dashboard.Helpers.Account.GetUserId(User.Identity.Name))</p>
                        </div>

                    </div>
                    <div style="min-height:100px;max-height:100px;">
                        @if (Dashboard.Helpers.Data.Any28D(Dashboard.Helpers.Account.GetUserId(User.Identity.Name)))
                        {
                            <canvas id="hours_month" height="100"></canvas>
                        }
                        else
                        {
                            <div class="card" style="height: 100px;">
                                <div class="card-body" style="text-align:center;">
                                    <i class="ti-bar-chart" style="font-size:50px;"></i><br />
